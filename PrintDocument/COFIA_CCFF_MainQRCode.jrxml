	<parameter name="C_Invoice_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[Select  numItem ,i.ad_Client_ID,i.dateinvoiced,ei_fechaEmi(i.c_Invoice_ID ) as fechaEmi,
to_Date(trim(ei_datereceived), 'DD/MM/YYYY HH24:MI:SS')  as datereceived,
   cantidad,
   uniMedida,
   (SELECT COALESCE(u.description, u.name, '') from c_INvoiceline il
   INNER JOIN M_Product p ON il.m_Product_ID=p.m_Product_ID
   INNER JOIN c_UOM u ON il.c_UOM_ID=u.c_uom_ID
   where il.c_Invoice_ID =i.c_INvoice_ID and  trim(j.codigo)= trim(p.value) LIMIT 1) as uom_Line,
   (SELECT il.priceentered from c_INvoiceline il
   INNER JOIN M_Product p ON il.m_Product_ID=p.m_Product_ID
   where il.c_Invoice_ID =i.c_INvoice_ID and  trim(j.codigo)= trim(p.value) LIMIT 1) as priceentered,
   (SELECT il.qtyentered from c_INvoiceline il
   INNER JOIN M_Product p ON il.m_Product_ID=p.m_Product_ID
   where il.c_Invoice_ID =i.c_INvoice_ID and  trim(j.codigo)= trim(p.value) LIMIT 1) as qtyentered,
   descripcion,
   precioUni::DECIMAL,
   montoDescu ::DECIMAL,
   ventaNoSuj::DECIMAL ,
  ventaExenta::DECIMAL, 
  ventaGravada::DECIMAL ,
  nogravado::DECIMAL ,
  codigo,
  i.c_Invoice_ID ,e_Invoiceelectronic_ID,
  i.ei_sellorecibido,
  i.ei_dateReceived,
  i.ei_codigogeneracion, ei_numerocontrol, 
  COALESCE(o.documentno, '') as o_Documentno, I.documentno, im.binarydata as logo,
  o.description as orderDescription, i.poreference
from
Json_CuerpoDocumento  j
INNER JOIN C_Invoice i ON i.c_INvoice_ID=j.c_INvoice_ID
INNER join ad_orginfo cf on i.ad_org_ID = cf.ad_org_ID
left join ad_image im on cf.logo_ID = im.ad_image_ID
LEFT JOIN C_Order o ON i.c_Order_ID=o.c_Order_ID
WHERE j.c_Invoice_ID=  $P{RECORD_ID} 
ORDER BY e_Invoiceelectronic_ID DESC]]>
	</queryString>
