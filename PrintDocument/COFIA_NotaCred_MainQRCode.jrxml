		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT 
binarydata as logo
FROM
ad_clientinfo cli
LEFT JOIN ad_image ai on cli.logo_ID = ai.ad_image_ID
WHERE cli.AD_Client_ID= $P{AD_CLIENT_ID}]]>
		</queryString>
		<field name="logo" class="byte[]">
			<property name="com.jaspersoft.studio.field.name" value="logo"/>
			<property name="com.jaspersoft.studio.field.label" value="logo"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="ad_image"/>
		</field>
	</subDataset>
	<parameter name="C_Invoice_ID" class="java.math.BigDecimal"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<parameter name="RECORD_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[Select  numItem ,COUNT(*) OVER() as total_rows_count,

i.ad_Client_ID,i.dateinvoiced,
   cantidad,
   ei_fechaEmi(i.c_Invoice_ID ) as fechaEmi,
to_Date(trim(ei_datereceived), 'DD/MM/YYYY HH24:MI:SS')  as datereceived,
   uniMedida,
    
   uomt.name as uom_Line,
   il_ordered.priceentered as priceentered,     
  il_ordered.qtyentered as qtyentered,
   descripcion,
   precioUni::DECIMAL,
   montoDescu ::DECIMAL,
   ventaNoSuj::DECIMAL ,
  ventaExenta::DECIMAL, 
  ventaGravada::DECIMAL ,
  nogravado::DECIMAL ,
  codigo,
  i.c_Invoice_ID ,e_Invoiceelectronic_ID,
  i.ei_sellorecibido,
  i.ei_dateReceived,
  i.ei_codigogeneracion, ei_numerocontrol, 
  COALESCE(o.documentno, '') as o_Documentno, I.documentno, im.binarydata as logo,
  COALESCE(o.description, '') as orderDescription, i.poreference
from
Json_CuerpoDocumento  j
INNER JOIN C_Invoice i ON i.c_INvoice_ID=j.c_INvoice_ID
INNER join ad_orginfo cf on i.ad_org_ID = cf.ad_org_ID
left join ad_image im on cf.logo_ID = im.ad_image_ID
LEFT JOIN C_Order o ON i.c_Order_ID=o.c_Order_ID
LEFT JOIN (
    SELECT *, 
    row_number() OVER (PARTITION BY c_invoice_id ORDER BY c_invoiceline_id ASC) as row_seq
    FROM c_invoiceline
) il_ordered ON il_ordered.c_invoice_id = i.c_invoice_id 
             AND il_ordered.row_seq = CAST(j.numItem AS INTEGER)
LEFT JOIN c_UOM_Trl uomt ON il_ordered.c_UOM_ID=uomt.c_UOM_ID AND uomt.ad_Language = 'es_SV'
WHERE j.c_Invoice_ID=  $P{RECORD_ID} 
ORDER BY e_Invoiceelectronic_ID]]>
	</queryString>
