	<queryString language="SQL">
		<![CDATA[SELECT 
Replace(receptor::jsonb->>'descActividad', '"', '') as descActividad,
Replace(receptor::jsonb->>'correo', '"', '') as correo,
Replace(receptor::jsonb->>'numDocumento', '"', '') as numDocumento,
Replace(receptor::jsonb->>'nit', '"', '') as nit,
Replace(receptor::jsonb->>'nrc', '"', '') as nrc,
Replace(receptor::jsonb->>'nombre', '"', '') as nombre,
Replace(receptor::jsonb->>'telefono', '"', '') as telefono,
Replace(receptor::jsonb->>'direccion', '"', '') as direccion,
Replace(receptor::json->'direccion'->>'complemento', '"', '') as complemento,
Replace(receptor::json->'direccion'->>'municipio', '"', '') as municipio,
ei_CityName(receptor::json->'direccion'->>'municipio',
		   receptor::json->'direccion'->>'departamento') as cityName, pt.name as paymentterm
,paymenttermduedate(i.c_paymentterm_id, i.dateinvoiced::timestamp with time zone) AS duedate

from e_electronicInvoice_Json ej
INNER JOIN C_Invoice i ON ej.c_Invoice_ID=i.c_Invoice_ID
INNER JOIN C_PaymentTerm pt ON i.c_Paymentterm_ID=pt.C_PaymentTerm_ID
WHERE ej.c_Invoice_ID=  $P{RECORD_ID} 
ORDER BY e_Invoiceelectronic_ID DESC
LIMIT 1]]>
