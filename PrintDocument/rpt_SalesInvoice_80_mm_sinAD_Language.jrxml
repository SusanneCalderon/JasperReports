	<subDataset name="Tipo de pago3" uuid="91bbfdc8-bbd2-45c5-8014-9fff9d334807">
		<parameter name="C_Invoice_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[$P{REPORT_CONTEXT}.getParameterValue("RECORD_ID")]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select
CASE
 when pa.tendertype= 'A' then 'ACH'
when pa.tendertype='C' then 'TDC'
when pa.tendertype='D' then 'DD'
when pa.tendertype='K' then 'CHQ'
when pa.tendertype='M' then 'CR'
when pa.tendertype='P' then 'PM'
when pa.tendertype='T' then 'CT'
when pa.tendertype='X' then 'CJ'
when pa.tendertype='Z' then 'ZLL'
end tendertype,
sum(pa.payamt) as amount,
ba.AccountNo,
COALESCE(b.Name, '') as Name,
pa.R_PnRef, pa.c_payment_id, cu.iso_code, cu.cursymbol,
CASE
when pa.tendertype='X' then ''
else pa.DocumentNo
end as DocumentNo
from  C_payment pa
LEFT JOIN  C_BankAccount ba ON (pa.C_BankAccount_ID=ba.C_BankAccount_ID)
LEFT JOIN  C_Bank b ON (b.C_Bank_ID=pa.C_Bank_ID)
LEFT JOIN  C_Currency cu ON (cu.C_Currency_ID= pa.C_Currency_ID)
WHERE pa.C_Invoice_ID=$P{C_Invoice_ID}
GROUP BY pa.tendertype,ba.AccountNo,b.Name,pa.R_PnRef,pa.c_payment_id,cu.iso_code, cu.cursymbol
ORDER BY pa.tendertype]]>
		</queryString>
		<field name="tendertype" class="java.lang.String"/>
		<field name="amount" class="java.math.BigDecimal"/>
		<field name="accountno" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
		<field name="r_pnref" class="java.lang.String"/>
		<field name="c_payment_id" class="java.math.BigDecimal"/>
		<field name="iso_code" class="java.lang.String"/>
		<field name="cursymbol" class="java.lang.String"/>
		<field name="documentno" class="java.lang.String"/>
	</subDataset>
	<parameter name="RECORD_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="C_Invoice_ID" class="java.math.BigDecimal" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{RECORD_ID}]]></defaultValueExpression>
	</parameter>
	<parameter name="ORACLE_REF_CURSOR" class="interface java.sql.ResultSet" isForPrompting="false"/>
	<parameter name="CURRENT_LANG" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT  

i.DocumentNo,
	i.DateInvoiced,
  	i.DocStatus,
  	i.Description,
  	i.Created,
  	COALESCE(co.DocumentNo, rma.DocumentNo) AS OrderDocumentNo,
   	i.DateOrdered,
  	dt.PrintName AS DocumentType,
   	dt.DocumentNote AS DocumentTypeNote,
  	i.TotalLines,
	ROUND((i.TotalLines * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedTotalLines,
    i.GrandTotal,
	ROUND((i.GrandTotal * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedGrandTotal,
    i.IsPaid,
    COALESCE(u.Name, '') AS SalesRepName,
    (CASE WHEN bp.IsEmployee = 'Y' THEN (COALESCE(bp.Name, '') || ' ' || COALESCE(bp.Name2, '')) ELSE bp.Name END) AS BPName,
    (CASE WHEN bp.IsEmployee = 'N' THEN (bp.Name2) ELSE null END) AS BPName2,
    COALESCE(bp.TaxID, bp.Value) AS BPTaxID,
    COALESCE(l.Address1, '') || COALESCE(', ' || l.Address2, '') || COALESCE(', ' || l.Address3, '') || COALESCE(', ' || l.Address4, '') || COALESCE(', ' || l.City, '') || COALESCE(' - ' || r.Name, '') BPAddress,
    COALESCE(dl.Address1, '') || COALESCE(', ' || dl.Address2, '') || COALESCE(', ' || dl.Address3, '') || COALESCE(', ' || dl.Address4, '') || COALESCE(', ' || dl.City, '') || COALESCE(' - ' || dl.RegionName, '') BPDeliveryAddress,
	COALESCE(mp.Value,'') AS ProductKey,
   	COALESCE(mp.Name, cch.Name, '') || COALESCE(' ' || cil.Description, '') AS ProductName,
    cil.Description LineDescription,
    SUM(cil.QtyEntered) QtyEntered,
    SUM(cil.QtyInvoiced) QtyInvoiced,
	COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1) AS rate,
	cct.name as conversionType,
    cil.PriceList PriceList,
	ROUND(((cil.PriceList * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1))), cc.stdprecision) AS convertedPriceList,
    cil.PriceEntered,
	ROUND((cil.PriceEntered * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedPriceEntered,
   	(CASE WHEN cil.PriceList = 0 THEN cil.PriceEntered ELSE cil.PriceList END) AS Price,
	(CASE WHEN cil.PriceList = 0
		 THEN ROUND((cil.PriceEntered * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision)
		 ELSE ROUND((cil.PriceList * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) END) AS convertedPrice,
 	cil.PriceActual,
	ROUND(((cil.PriceActual) * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)) ,cc.stdprecision) AS convertedPriceActual,
 	ABS(((cil.PriceActual - cil.PriceList) / CASE WHEN cil.PriceList = 0 THEN 1 ELSE cil.PriceList END) * 100) Discount,
	sum(cil.LineNetAmt) LineNetAmt,
	ROUND((sum(cil.LineNetAmt)  * COALESCE(currencyrate(i.c_currency_id, olc.c_currency_id, i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedLineNetAmt,
    sum(cil.LineNetAmt + (cil.LineNetAmt * ct.Rate)) LineTotalAmt,
    cc.CurSymbol AS Currency,
	ccb.CurSymbol as ConvertedCurrency,
	asd.C_Currency_ID as acctSchemaCurrency,
	i.C_Currency_ID as invoiceCurrency,
	ct.Name AS Tax,
	ct.TaxIndicator,
 	(i.DateInvoiced + pt.NetDays) DueDate,
  	u.Description SalesRep,
 	bpl.Phone,
  	dt.DocBaseType,
  	pt.Name PaymentTerm,
  	--COALESCE(umtrl.UOMSymbol, uom.UOMSymbol) as UOMWeight,
 	mp.Weight as ProductWeight,
 	i.POReference,
 	mp.UnitsPerPack,
  	mp.UnitsPerPallet,
	
        org.Name as OrgName,
	    COALESCE(ol.Address1, '') || COALESCE(', ' || ol.Address2, '') || COALESCE(', ' || ol.Address3, '') || COALESCE(', ' || ol.Address4, '') || COALESCE(', ' || ol.City, '') || COALESCE(' - ' || olr.Name, '') OrgAddress,
	    oi.TAXID as OrgTAXID,
	    wa.Name as Warehouse,
(SELECT 
SUM(CASE
        WHEN um.stdprecision = 0 AND il.M_Product_ID > 0 THEN il.Qtyentered
		ELSE il.Qtyentered * 1
    END)
FROM C_InvoiceLine il
LEFT JOIN M_Product p ON (p.M_Product_ID = il.M_Product_ID)
LEFT JOIN C_UOM um ON (p.C_UOM_ID = um.C_UOM_ID)
WHERE il.c_invoice_id =  $P{RECORD_ID} ) AS ProductQuantities,
(SELECT 
COUNT(il.C_InvoiceLine_ID)
FROM C_InvoiceLine il
WHERE il.c_invoice_id =  $P{RECORD_ID} ) AS invocielinesqty,
cam.name as Channel,

pos.CashDrawer,
c.name as razaosocial,
c.description as giro,
oi.duns,
    pos.posAuthorizationCode, pos.posResolutionNo, pos.ticketNoFrom, pos.ticketNoTo, pos.startdate, dt.description as doctypedescription, oi.printname as orgprintname, ci.printname as clientprintname, oi.phone as orgphone,
   COALESCE(shw_searchtaxamt(i.c_Invoice_ID, 'IVA') + shw_searchtaxbaseamt(i.c_Invoice_ID, 'IVA'), 0.00) as ventaGravada,
   COALESCE(shw_searchtaxamt(i.c_Invoice_ID, 'EXT') + shw_searchtaxbaseamt(i.c_Invoice_ID, 'EXT'), 0.00)  as ventaExenta,
   COALESCE(shw_searchtaxamt(i.c_Invoice_ID, 'NSUJ') + shw_searchtaxbaseamt(i.c_Invoice_ID, 'NSUJ'),0.00)  as ventaNoSujeta,	
   COALESCE(shw_discountAmt(i.c_Invoice_ID),0.00) AS discountamt, i.c_Order_ID, ci.slogan, getdate() as timenow
   
   FROM C_Invoice i
   INNER JOIN C_DocType dt  ON(dt.C_DocType_ID = i.C_DocType_ID)
   INNER JOIN C_InvoiceLine cil ON(i.C_Invoice_ID = cil.C_Invoice_ID)
   INNER JOIN C_BPartner bp ON(i.C_BPartner_ID = bp.C_BPartner_ID)
   LEFT JOIN C_BPartner_Location bpl ON(i.C_BPartner_Location_ID = bpl.C_BPartner_Location_ID)
   LEFT JOIN C_Location l ON(bpl.C_Location_ID = l.C_Location_ID)
   LEFT JOIN C_Region r ON (r.C_Region_ID=l.C_Region_ID)
   INNER JOIN AD_ClientInfo ci ON(ci.AD_Client_ID = i.AD_Client_ID)
   INNER JOIN AD_Client c ON (i.AD_Client_ID=c.AD_Client_ID)
   INNER JOIN C_AcctSchema asd ON (ci.c_acctschema1_id = asd.c_acctschema_id)   
   LEFT JOIN C_ConversionType cct ON (i.C_ConversionType_ID = cct.C_ConversionType_ID)
   
   LEFT JOIN C_UOM uom ON(uom.C_UOM_ID = cil.C_UOM_ID)
   LEFT JOIN C_UOM_TRL umtrl on (uom.C_UOM_ID = umtrl.C_UOM_ID)
   --LEFT JOIN C_UOM uomr ON(uomr.C_UOM_ID = ci.Report_UOM_Weight_ID)
   LEFT JOIN AD_User u ON(u.AD_User_ID = i.SalesRep_ID)
   LEFT JOIN AD_User cu ON(cu.AD_User_ID = i.CreatedBy)
   --LEFT JOIN C_Invoice da ON(da.C_Invoice_ID = i.InvoiceToAllocate_ID)
   LEFT JOIN C_PaymentTerm pt ON(i.C_PaymentTerm_ID = pt.C_PaymentTerm_ID)
   LEFT JOIN C_Currency cc ON(i.C_Currency_ID = cc.C_Currency_ID)
   LEFT JOIN C_Order co ON(i.C_Order_ID = co.C_Order_ID)
   LEFT JOIN M_Warehouse wa ON (co.M_Warehouse_ID=wa.M_Warehouse_ID)
   LEFT JOIN C_POS pos ON (co.C_POS_ID=pos.C_POS_ID)   
   LEFT JOIN M_RMA rma ON(rma.M_RMA_ID = i.M_RMA_ID)
   LEFT JOIN C_BPartner_Location dbpl ON(co.C_BPartner_Location_ID = dbpl.C_BPartner_Location_ID)
   LEFT JOIN C_Location dl ON(dbpl.C_Location_ID = dl.C_Location_ID)
   LEFT JOIN M_Product mp ON(cil.M_Product_ID = mp.M_Product_ID)
   LEFT JOIN C_Charge cch ON(cil.C_Charge_ID = cch.C_Charge_ID)
   LEFT JOIN C_Tax ct ON(cil.C_Tax_ID = ct.C_Tax_ID)
   LEFT JOIN (SELECT il.C_Invoice_ID, SUM(p.Weight * il.QtyInvoiced) Weight
        FROM C_InvoiceLine il
        LEFT JOIN M_Product p ON(p.M_Product_ID = il.M_Product_ID)
        GROUP BY il.C_Invoice_ID) pw ON(pw.C_Invoice_ID = i.C_Invoice_ID)
        LEFT JOIN AD_Org org On (org.AD_Org_ID = i.AD_Org_ID)
        LEFT JOIN AD_OrgInfo oi ON(oi.AD_Org_ID = org.AD_Org_ID)
        LEFT JOIN C_Location ol ON(ol.C_Location_ID = oi.C_Location_ID)
        LEFT JOIN C_Region olr ON (olr.C_Region_ID=ol.C_Region_ID)
        LEFT JOIN C_Country olc ON (olc.C_Country_ID=ol.C_Country_ID)
        LEFT JOIN C_Currency ccb ON(olc.c_currency_id = ccb.C_Currency_ID)
        LEFT JOIN M_Shipper oms ON(oms.M_Shipper_ID = co.M_Shipper_ID)
        LEFT JOIN C_Campaign cam  ON(cam.C_Campaign_ID = i.C_Campaign_ID)
WHERE i.C_Invoice_ID=  $P{RECORD_ID} 
AND umtrl.AD_Language ='es_SV'
GROUP BY ProductKey,ProductName,LineDescription,i.grandtotal,i.ispaid, SalesRepName,bp.name, 
bp.name2,BPTaxID,l.address1,l.address2,l.address3,l.address4,l.city,l.regionname, i.documentno,
i.dateinvoiced,i.docstatus,i.description,OrderDocumentNo,i.dateordered, DocumentType,DocumentTypeNote, 
i.TotalLines,cil.PriceList,cil.PriceEntered,Price,cil.PriceActual,Discount, Currency, 
Tax, ct.TaxIndicator, DueDate, SalesRep, bpl.Phone, dt.DocBaseType, PaymentTerm,  ProductWeight, i.POReference, 
mp.UnitsPerPack, mp.UnitsPerPallet,  i.c_currency_id, asd.c_currency_id, i.c_conversiontype_id, 
i.ad_client_id, i.ad_org_id, cc.stdprecision , ccb.cursymbol , cct.name,cu.Name,r.name,bp.isemployee,dbpl.name, 
dl.Address1, dl.Address2, dl.Address3, dl.Address4, dl.City,dl.RegionName, org.Name,ol.Address1, ol.Address2, 
ol.Address3, ol.Address4, ol.City, olr.Name, oi.TAXID,wa.Name,olc.c_currency_id, oms.Name,cam.name,i.Created,
mp.C_TaxCategory_ID, pos.cashdrawer,c.name,c.description,oi.duns,pos.posAuthorizationCode,pos.posResolutionNo,pos.ticketNoFrom, pos.ticketNoTo, pos.startdate,dt.description,orgprintname,clientprintname,orgphone, i.c_Invoice_ID,slogan,timenow
ORDER BY ProductKey;]]>
	</queryString>
