	<subDataset name="Total" uuid="21fda901-2651-4150-8b2d-3e456d120cfc">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Impala (LVE)"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="C_Invoice_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[$P{REPORT_CONTEXT}.getParameterValue("RECORD_ID")]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
ROUND((cit.TaxBaseamt * COALESCE(currencyrate(i.c_currency_id, asd.c_currency_id, COALESCE(da.dateinvoiced, i.dateinvoiced), i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedTaxBaseamt,
cit.TaxBaseamt,
cit.TaxAmt,
ROUND((cit.TaxAmt * COALESCE(currencyrate(i.c_currency_id, asd.c_currency_id, COALESCE(da.dateinvoiced, i.dateinvoiced), i.c_conversiontype_id, i.ad_client_id, i.ad_org_id), 1)),cc.stdprecision) AS convertedTaxAmt,
COALESCE(t.Description, t.Name) AS Tax,
cc.CurSymbol AS Currency,
ccb.CurSymbol as ConvertedCurrency,
t.TaxIndicator
FROM C_InvoiceTax cit
INNER JOIN C_Tax t ON(t.C_Tax_ID = cit.C_Tax_ID)
INNER JOIN C_Invoice i ON(i.C_Invoice_ID = cit.C_Invoice_ID)
INNER JOIN AD_ClientInfo ci ON(ci.AD_Client_ID = i.AD_Client_ID)
INNER JOIN C_AcctSchema asd ON (ci.c_acctschema1_id = asd.c_acctschema_id)
INNER JOIN C_Currency cc ON(i.C_Currency_ID = cc.C_Currency_ID)
--LEFT JOIN C_Conversion_Rate ccr ON(i.C_Currency_ID = ccr.C_Currency_ID and i.C_ConversionType_ID = ccr.C_ConversionType_ID)
LEFT JOIN C_Currency ccb ON(asd.C_Currency_ID = ccb.C_Currency_ID)
LEFT JOIN C_Invoice da ON(da.C_Invoice_ID = i.InvoiceToAllocate_ID)
WHERE cit.C_Invoice_ID = $P{C_Invoice_ID}
GROUP BY cit.TaxBaseAmt, cit.TaxAmt, t.Name,cc.CurSymbol,t.Description,i.c_currency_id, asd.c_currency_id,i.dateinvoiced, i.c_conversiontype_id, i.ad_client_id, i.ad_org_id, cc.stdprecision, ccb.CurSymbol,t.TaxIndicator,da.dateinvoiced
ORDER BY t.Name ASC]]>
		</queryString>
		<field name="taxbaseamt" class="java.math.BigDecimal"/>
		<field name="taxamt" class="java.math.BigDecimal"/>
		<field name="tax" class="java.lang.String"/>
		<field name="currency" class="java.lang.String"/>
		<field name="convertedTaxBaseamt" class="java.math.BigDecimal"/>
		<field name="convertedTaxAmt" class="java.math.BigDecimal"/>
		<field name="ConvertedCurrency" class="java.lang.String"/>
		<field name="TaxIndicator" class="java.lang.String"/>
	</subDataset>
