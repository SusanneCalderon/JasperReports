bp.value, bp.name, coalesce(l.address1, '') as address1,
coalesce(l.address2, '') as address2, coalesce(l.city, '') as city , coalesce(c.name, '') as countryname,
coalesce(bpc.name, '') as contactname, coalesce(bpc.email, '') as email, 
