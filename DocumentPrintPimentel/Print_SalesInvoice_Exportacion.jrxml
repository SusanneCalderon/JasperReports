	<queryString>
		<![CDATA[select ilt.uomsymbol, ilt.ventaex, ilt.priceactual, ilt.discount, ilt.productvalue, ilt.pricelist, 
ilt.qtyinvoiced, ilt.linenetamt, ilt.name,
coalesce(pp.name,' ') as phasename, coalesce(pt.name,' ') as taskname, 
ilt.productdescription

 from shw_c_invoice_linetax_vt_detail ilt
left join C_ProjectPhase pp on ilt.C_ProjectPhase_ID = pp.C_ProjectPhase_ID
left join C_ProjectTask pt on ilt.C_ProjectTask_ID = pt.C_ProjectTask_ID
where ilt.c_invoice_id = $P{RECORD_ID}
and ilt.ad_language = 'es_SV'
order by ilt.line]]>
	</queryString>
