		<queryString>
			<![CDATA[select
 sum(totallines ) as totallines ,
 sum(grandtotal) as grandtotal,
 sum(ventagravada) as ventagravada,
 sum(taxiva) as taxiva,
 sum(ventnosujeta) as ventnosujeta,
 sum(ventaext) as ventaext,
 sum(taxret) as taxret,
