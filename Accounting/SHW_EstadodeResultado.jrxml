		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="AD_USER_ID" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="C_Period_ID" class="java.math.BigDecimal"/>
	<parameter name="GL_Journal_ID" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select t.seqno, t.name,t.description,col_0, t.pa_reportline_id, rl.linetype,rl.tablevel,t.levelno,
getdate() as getdate
, (select name from ad_user where ad_user_ID =$P{AD_USER_ID}) as user,
(select enddate from c_period where ad_client_ID= $P{AD_CLIENT_ID}
and c_period_ID = $P{C_Period_ID}) as enddate
 from t_report t
 left join pa_reportline rl on t.pa_reportline_ID = rl.pa_reportline_ID

where t.ad_pinstance_ID = $P{T_Report_AD_PInstance_ID} AND rl.IsPrinted = 'Y'
UNION ALL
select  jl.seqno + 1000000 as seqno, 
CASE WHEN ev.name = 'Del presente Ejercicio' THEN '= ' || ev.value else
'- ' || ev.value end as name, 
ev.name as description, 
acctbalance(account_ID, amtacctdr, amtacctcr) As col_0,
null as pa_reportline_id, 
CASE WHEN ev.name = 'Del presente Ejercicio' THEN 'C' ELSE 'S' END AS linetype, 
0 as tablevel, 0 as levelno, getdate() as getdate,
(select name from ad_user where ad_user_ID =  $P{AD_USER_ID}) as user,
(select enddate from c_period where c_period_ID =  $P{C_Period_ID} ) as enddate

from gl_JOurnalline jl 
INNER JOIN c_Elementvalue ev ON jl.account_ID=ev.c_elementvalue_ID
where gl_Journal_ID= $P{GL_Journal_ID}  and seqno is not null]]>
	</queryString>
